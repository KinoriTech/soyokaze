{
    "name": "kinoritech/soyokaze",
    "description": "Minimal Laravel authentication scaffolding with Blade and Metro 4.",
    "keywords": ["laravel", "auth", "metro"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/KinoriTech/soyokaze/issues",
        "source": "https://github.com/KinoriTech/soyokaze"
    },
    "authors": [
        {
            "name": "Horacio Hoyos Rodriguez",
            "email": "h.hoyos.r@kinori.tech"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/filesystem": "^8.42",
        "illuminate/support": "^8.42",
        "illuminate/validation": "^8.42"
    },
    "autoload": {
        "psr-4": {
            "KinoriTech\\Soyokaze\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "KinoriTech\\Soyokaze\\SoyokazeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
